xport default function UsersList() {\n  const classes = useStyles();\n  const dispatch = useDispatch();\n\n  const [error, setError] = useState(false);\n  const [page, setPage] = useState(1);\n  useEffect(() => {\n    dispatch(getUsersThunk(page, setError));\n    scroll.scrollToTop();\n  }, [page]);\n\n  const usersList = useSelector((state) => state.userList);\n\n  const handleClick = (evt, value) => {\n    // VERIFIES IF THERE IS CONTENT IN THE NEXT PAGE\n    axios.get(\"https://kenziehub.me/users?page=\" + value).then((res) => {\n      res.data.length && setPage(value);\n    });\n  };\n\n  return (\n    <div className={classes.userslistContainer}>\n      {error ? (\n        <p>Ocorreu algum erro</p>\n      ) : (\n        usersList.map((user, index) => {\n          return <UserCard user={user} key={index} />;\n        })\n      )}\n      <div className={classes.root}>\n        <Pagination\n          page={page}\n          onChange={handleClick}\n          siblingCount={-1}\n          count={page + 1}\n          variant=\"outlined\"\n          shape=\"rounded\"\n        />\n      </div>\n    </div>\n  );\n}\n",["105","106"],"/home/luizmatheusga/Área de Trabalho/q2/sprint4/project/kenziehub/src/components/login/Login.jsx",[],["107","108"],"/home/luizmatheusga/Área de Trabalho/q2/sprint4/project/kenziehub/src/store/modules/token/reducer.js",[],"/home/luizmatheusga/Área de Trabalho/q2/sprint4/project/kenziehub/src/store/modules/users-list/thunk.js",[],"/home/luizmatheusga/Área de Trabalho/q2/sprint4/project/kenziehub/src/components/user-card/UserCard.jsx",[],"/home/luizmatheusga/Área de Trabalho/q2/sprint4/project/kenziehub/src/components/register/Register.jsx",[],"/home/luizmatheusga/Área de Trabalho/q2/sprint4/project/kenziehub/src/pages/new-job/index.jsx",[],"/home/luizmatheusga/Área de Trabalho/q2/sprint4/project/kenziehub/src/index.js",[],"/home/luizmatheusga/Área de Trabalho/q2/sprint4/project/kenziehub/src/store/index.js",[],"/home/luizmatheusga/Área de Trabalh